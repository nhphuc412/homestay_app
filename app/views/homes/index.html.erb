<p id="notice"><%= notice %></p>

<h1>Homes</h1>

<table>
  <thead>
    <tr>
      <th>Home name</th>
      <th>Home type</th>
      <th>M pref</th>
      <th>Description</th>
      <th>Image</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @homes.each do |home| %>
      <tr>
        <td><%= home.home_name %></td>
        <td><%= home.home_type_id %></td>
        <td><%= home.m_pref_id %></td>
        <td><%= home.description %></td>
        <td><%= home.image %></td>
        <td><%= home.status %></td>
        <td><%= link_to 'Show', home %></td>
        <td><%= link_to 'Edit', edit_home_path(home) %></td>
        <td><%= link_to 'Destroy', home, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Home', new_home_path %>
